#!/bin/sh

tflag="--template=$topdir/template-hsc.h"
Iflag="-I$topdir/include/"
for arg do
    case "$arg" in
        -c*)          HSC2HS_EXTRA=;;
        --cc=*)       HSC2HS_EXTRA=;;
        -t*)          tflag=;;
        --template=*) tflag=;;
        --)           break;;
    esac
done

exec "$executablename" "$tflag" $HSC2HS_EXTRA ${1+"$@"} "$Iflag"
